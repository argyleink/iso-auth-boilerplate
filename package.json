  "scripts": {
    "server-only": "NODE_ENV=development && REACT_SERVER_RENDER=true && REACT_CLIENT_RENDER=false && DEBUG=*,-babel,-express*,-socket.io*,-engine*,-connect*,-morgan,-Fluxible*,-send,-eslint* && gulp server-only --serverOnly",
    "client": "NODE_ENV=development && REACT_SERVER_RENDER=false && DEBUG=*,-babel,-express*,-socket.io*,-engine*,-connect*,-morgan,-Fluxible*,-send,-eslint* && gulp dev --clientOnly",
    "iso": "NODE_ENV=development && REACT_SERVER_RENDER=true && DEBUG=*,-babel,-express*,-socket.io*,-engine*,-connect*,-morgan,-Fluxible*,-send,-eslint* && gulp dev --iso",
    "prod": "NODE_ENV=production && REACT_SERVER_RENDER=true && DEBUG=-* && gulp build && node server",
    "setup": "DEBUG=*,-babel && node ./setup.js",
    "start": "npm cache clean && npm install && npm run setup --no-spin && npm run client"
  },
  "main": "server.js",
  "repository": {
    "type": "git",
